
<div class="jumbotron">
  <h1>Artists</h1>
</div>
<div class="row">
  <div class="col-sm-6 col-md-4">
    <% @artists.each do |artist| %>
    <div class="thumbnail">
      <%= image_tag artist.image_url %>
      <div class="caption">
        <h3><%= link_to artist.name, artist_path(artist.id) %></h3>
      </div>
    </div>
    <% end %>
  </div>
</div>
